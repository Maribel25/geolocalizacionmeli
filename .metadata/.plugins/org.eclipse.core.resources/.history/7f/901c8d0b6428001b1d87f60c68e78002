version: "3"
services:
  mysql_container:
    image: mysql:8.0
    environment:
      - MYSQL_ROOT_PASSWORD=secret
      - MYSQL_DATABASE=geo_schema
      - MYSQL_USER=mysqluser
      - MYSQL_PASSWORD=secret
    ports:
      - 13306:13306
      
  geolocalizacionips:
    image: geolocalizacionips:latest
    ports:
      - 8080:8080
    build:
      context: ./
      dockerfile: Dockerfile
    depends-on:
      -  mysql_container